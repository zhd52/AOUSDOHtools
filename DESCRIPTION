Package: AOUSDOHtools
Type: Package
Title: Analyzing AOU SDOH Survey Data
Version: 1.0.4
Authors@R: c(
    person("Zhirui", "Deng", email = "zhd52@pitt.edu", role = c("aut", "cre")),
    person("Theresa A.", "Koleck", role = "ctb"),
